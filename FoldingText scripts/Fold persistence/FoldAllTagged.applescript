property pTitle : "Fold all nodes tagged with @fold"property pVer : "0.01"property pAuthor : "Robin Trew"property pstrTag : "fold"on run	tell application "FoldingText"		set lstDocs to documents		if lstDocs â‰  {} then			tell item 1 of lstDocs				set strIDs to body of (read JSON (HTTP request URI "/nodes.json?path=//@" & pstrTag & "&attributes=id"))				HTTP request method "PUT" URI "/view/collapsed.json" body strIDs			end tell		end if	end tellend run